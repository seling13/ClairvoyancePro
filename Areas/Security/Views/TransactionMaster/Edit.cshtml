@model OneFin.Models.TransactionMaster

@{
    ViewData["Title"] = "Edit";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        <i class="fa fa-bell"></i> Option Master
    </h1>
    <ol class="breadcrumb">
        <li><a href="/home/index"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="~/Security/TransactionMaster/index"><i class="fa fa-cog"></i> Role Master</a></li>
        <li class="active"> Edit</li>
    </ol>
</section>

<section class="content container-fluid">
    <!-- Horizontal Form -->
    <div class="box box-info">
        <div class="box-header with-border">
            <h3 class="box-title">Edit</h3>
        </div>
        <!-- /.box-header -->
        <!-- form start -->
        <form asp-action="Edit" class="form-horizontal">
            <div class="box-body">                    
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <input type="hidden" asp-for="ID" />                   
                <div class="form-group">                      
                    <label asp-for="Module" class="col-sm-2 control-label"></label>                       
                    <div class="col-sm-10">                           
                        <input asp-for="Module" class="form-control" />                            
                        <span asp-validation-for="Module" class="text-danger"></span>                       
                    </div>                        
                </div>                       
                <div class="form-group">                         
                    <label asp-for="TransactionName" class="col-sm-2 control-label">Transaction Name</label>                           
                    <div class="col-sm-10">                                
                        <input asp-for="TransactionName" class="form-control" />                                
                        <span asp-validation-for="TransactionName" class="text-danger"></span>                                                          
                    </div>
                </div>                           
                <div class="form-group">                                
                    <label asp-for="Role" class="col-sm-2 control-label"> User Role</label>                                
                    <div class="col-sm-10">                                    
                        <input asp-for="Role" class="form-control" />                                    
                        <span asp-validation-for="Role" class="text-danger"></span>                                    
                    </div>                                     
                </div>
                <div class="box-footer">                                    
                    <div class="form-group">                                        
                        <a asp-action="Index">Back to List</a>    
                        <input type="submit" value="Save" class="btn btn-info pull-right" />                                    
                    </div>                               
                </div>                        
            </div>                
        </form>        
    </div>
            <!-- /.box -->
</section>