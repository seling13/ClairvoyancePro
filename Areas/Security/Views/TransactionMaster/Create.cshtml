@model OneFin.Models.TransactionMaster

@{
    ViewData["Title"] = "Create";
}

<section class="content-header">
    <h1>
        <i class="fa fa-list"></i> Transaction Master
    </h1>
    <ol class="breadcrumb">
        <li><a href="/home/index"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="~/Security/TransactionMaster/index"><i class="fa fa-bell"></i> Transaction Master</a></li>
        <li class="active"> Create</li>
    </ol>
</section>
<section class="content container-fluid">
    <!-- Horizontal Form -->
    <div class="box box-info">
        <div class="box-header with-border">
            <h3 class="box-title">Create</h3>
        </div>
        <!-- /.box-header -->
        <!-- form start -->
        <form asp-action="Create" class="form-horizontal">
            <div class="box-body">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Module" class="col-sm-2 control-label"></label>
                    <div class="col-sm-10">
                        <input asp-for="Module" class="form-control" />
                        <span asp-validation-for="Module" class="text-danger"></span>
                    </div>
                    </div>
                <div class="form-group">
                    <label asp-for="TransactionName" class="col-sm-2 control-label">Transaction Name</label>
                    <div class="col-sm-10">
                        <input asp-for="TransactionName" class="form-control" />
                        <span asp-validation-for="TransactionName" class="text-danger"></span>
                        </div>
                    </div>
                <div class="form-group">
                    <label asp-for="Role" class="col-sm-2 control-label">User Role</label>
                    <div class="col-sm-10">
                        @Html.DropDownList("Role", new SelectList(ViewBag.data, "Name", "NormalizedName"))
                        <span asp-validation-for="Role" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="box-footer">
                        <a asp-action="Index">Back to List</a>
                        <!--<button type="button" asp-action="Index" class="btn btn-default">Cancel</button>-->
                        <button type="submit" class="btn btn-info pull-right">Create</button>
                    </div>
                </div>
        </form>
    </div>
    <!-- /.box -->
</section>