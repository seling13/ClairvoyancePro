            -webkit-animation-duration: 0.6s;
            animation-name: animatebottom;
            animation-duration: 0.6s
        }

        .animate-transition {
            position: absolute;
            -webkit-animation-name: animatetransition;
            -webkit-animation-duration: 1s;
            animation-name: animatetransition;
            animation-duration: 1s
        }

        .animate-transition-relative {
            position: relative;
            -webkit-animation-name: animatetransition;
            -webkit-animation-duration: 1s;
            animation-name: animatetransition;
            animation-duration: 2s
        }

        .animate-leftright {
            position: relative;
            -webkit-animation-name: animataleftright;
            -webkit-animation-duration: 0.6s;
            animation-name: animataleftright;
            animation-duration: 0.6s
        }

        @@-webkit-keyframes animatebottom {
            from {
                bottom: -100px;
                opacity: 0
            }

            to {
                bottom: 0px;
                opacity: 1
            }
        }

